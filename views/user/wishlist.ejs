								<td class="price-col">â‚¹ <%= key.productId.price %></td>
								<td class="stock-col">
                                    <%if(key.productId.stock==0){%>
                                        <span class="out-of-stock">Out of stock</span>

                                        <%}else{%>
                                            <span class="in-stock">In stock</span>

                                   <% }%>
                                </td>
								<!-- <td class="stock-col"><span class="out-of-stock">Out of stock</span></td> -->

								<td class="action-col">
                                    <%if(key.productId.stock==0){%>
                                        <button class="btn btn-block btn-outline-primary-2 disabled">Out of Stock</button>
                                        <%}else{%>
								      <a href="/add-to-cart-delete-wishlist?id=<%= key.productId._id %>"><button class="btn btn-block btn-outline-primary-2"><i class="icon-cart-plus"></i>Add to Cart</button></a>
                                   <% }%>

								</td>
								<td class="remove-col"><a href="/delete-wishlist?id=<%= key.productId._id %>"><button class="btn-remove"><i class="icon-close"></i></button></a></td>
							</tr>
                            <% } }else{%>
                                <tr>
                                    <td colspan="5"><h3>Wishlist is Empty</h3> </td>
                                </tr>
                                <%}%>


                            <% }else{%>
